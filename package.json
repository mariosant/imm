{
  "name": "@mariosant/imm",
  "version": "0.4.1",
  "description": "Develop reducers easier",
  "main": "dist/index.js",
  "scripts": {
    "lint": "xo",
    "test": "jest",
    "build": "parcel build src/index.js --experimental-scope-hoisting --no-source-maps"
  },
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-ramda": "^2.0.0",
    "husky": "^1",
    "jest": "^23",
    "lint-staged": "^8",
    "parcel-bundler": "^1.11.0",
    "xo": "^0.24"
  },
  "lint-staged": {
    "*": [
      "xo --fix",
      "git add"
    ]
  },
  "xo": {
    "prettier": true,
    "envs": [
      "jest",
      "node",
      "browser"
    ],
    "rules": {
      "camelcase": 1
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
