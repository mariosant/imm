{
  "name": "@mariosant/imm",
  "version": "0.6.0",
  "description": "Develop reducers easier",
  "main": "dist/index.js",
  "scripts": {
    "lint": "xo",
    "test": "jest",
    "build": "parcel build src/index.js --experimental-scope-hoisting --no-source-maps"
  },
  "license": "MIT",
  "dependencies": {
    "ramda": "^0"
  },
  "devDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "babel-jest": "^24",
    "babel-plugin-ramda": "^2",
    "husky": "^1",
    "jest": "^24",
    "lint-staged": "^8",
    "parcel-bundler": "^1",
    "xo": "^0"
  },
  "lint-staged": {
    "*": [
      "xo --fix",
      "git add"
    ]
  },
  "xo": {
    "prettier": true,
    "envs": [
      "jest",
      "node",
      "browser"
    ],
    "rules": {
      "camelcase": 1
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
